      </TextField>
      <TextField maxWidth="300.0" promptText="Email">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </TextField>
      <TextField maxWidth="300.0" promptText="Password">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </TextField>
      <Button mnemonicParsing="false" text="Login" textFill="WHITE">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" left="30.0" right="30.0" top="10.0" />
         </padding>
      </Button>
      <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Non hai un Account? Registrati." textAlignment="CENTER">
         <VBox.margin>
            <Insets top="10.0" />
